{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "dependsOn": [
        "^build"
      ]
    },
    "test": {
      "dependsOn": [
        "^build",
        "build"
      ],
      "outputs": []
    },
    "test:ci": {
      "dependsOn": [
        "^build",
        "build"
      ],
      "outputs": []
    },
    "format": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "lint": {
      "dependsOn": [
        "^build"
      ],
      "cache": false
    },
    "typecheck": {
      "cache": false
    }
  }
}