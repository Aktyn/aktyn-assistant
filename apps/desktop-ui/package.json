    "path-browserify": "^1.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-scripts": "5.0.1",
    "showdown": "^2.1.0",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0"
