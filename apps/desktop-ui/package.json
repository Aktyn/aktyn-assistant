{
  "name": "@aktyn-assistant/desktop-ui",
  "version": "2.2.0",
  "private": true,
  "homepage": "./",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "ui:add": "npx shadcn@latest add",
    "lint": "eslint \"src/**/*{.ts,.tsx}\" --fix",
    "format": "prettier --write src/**/*{.ts,.tsx}",
    "typecheck": "tsc --noEmit --skipLibCheck -p ./tsconfig.app.json",
    "test": "vitest run --passWithNoTests",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@lucide/lab": "^0.1.2",
    "@radix-ui/react-checkbox": "^1.2.3",
    "@radix-ui/react-dialog": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.12",
    "@radix-ui/react-label": "^2.1.4",
    "@radix-ui/react-popover": "^1.1.11",
    "@radix-ui/react-scroll-area": "^1.2.6",
    "@radix-ui/react-select": "^2.2.2",
    "@radix-ui/react-separator": "^1.1.4",
    "@radix-ui/react-slot": "^1.2.0",
    "@radix-ui/react-tabs": "^1.1.9",
    "@radix-ui/react-tooltip": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "globals": "^16.0.0",
    "lucide-react": "^0.503.0",
    "next-themes": "^0.4.6",
    "path-browserify": "^1.0.1",
    "react": "^19.1.0",
    "react-compiler-runtime": "beta",
    "react-dom": "^19.1.0",
    "showdown": "^2.1.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.2.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^22.15.2",
    "@types/path-browserify": "^1.0.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/showdown": "^2",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-plugin-react-compiler": "experimental",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-compiler": "experimental",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-typescript": "^1.1.0",
    "prettier": "^3.5.3",
    "shadcn-ui": "^0.9.5",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "vite": "^6.3.3"
  }
}
